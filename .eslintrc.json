{
    "extends": "airbnb-base",
    "plugins": [
        "jest",
        "import"
    ],
    "env": {
        "jest/globals": true
    },
    "rules": {
        "import/prefer-default-export":"off",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/*.test.js"]}]
    }
}
